{
  "name": "xendit-medusa",
  "version": "0.1.0",
  "description": "Xendit payment provider plugin for Medusa v2. Accept payments from Southeast Asian payment methods including Indonesian e-wallets (OVO, DANA, LinkAja, ShopeePay), virtual accounts (BCA, BNI, BRI, Mandiri), QR codes (QRIS), and cards. Implements Xendit Payments API v3 with full webhook support.",
  "author": "MuhamadAjiW",
  "repository": {
    "type": "git",
    "url": "https://github.com/MuhamadAjiW/xendit-medusa.git"
  },
  "license": "MIT",
  "main": "./.medusa/server/src/index.js",
  "files": [
    ".medusa/server"
  ],
  "exports": {
    ".": "./.medusa/server/src/index.js",
    "./package.json": "./package.json",
    "./workflows": "./.medusa/server/src/workflows/index.js",
    "./.medusa/server/src/modules/*": "./.medusa/server/src/modules/*/index.js",
    "./modules/*": "./.medusa/server/src/modules/*/index.js",
    "./providers/*": "./.medusa/server/src/providers/*/index.js",
    "./*": "./.medusa/server/src/*.js",
    "./admin": {
      "import": "./.medusa/server/src/admin/index.mjs",
      "require": "./.medusa/server/src/admin/index.js",
      "default": "./.medusa/server/src/admin/index.js"
    }
  },
  "keywords": [
    "medusa",
    "plugin",
    "medusa-plugin-payment",
    "medusa-plugin-integration",
    "medusa-plugin",
    "medusa-v2",
    "xendit",
    "payment-gateway",
    "indonesia",
    "southeast-asia"
  ],
  "scripts": {
    "build": "medusa plugin:build",
    "compile": "medusa plugin:build",
    "dev": "medusa plugin:develop",
    "prepublishOnly": "medusa plugin:build"
  },
  "devDependencies": {
    "@medusajs/admin-sdk": "2.11.2",
    "@medusajs/cli": "2.11.2",
    "@medusajs/framework": "2.11.2",
    "@medusajs/medusa": "2.11.2",
    "@medusajs/test-utils": "2.11.2",
    "@medusajs/ui": "4.0.25",
    "@medusajs/icons": "2.11.2",
    "@medusajs/admin-shared": "2.11.2",
    "@swc/core": "1.5.7",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53"
  },
  "peerDependencies": {
    "@medusajs/admin-sdk": "2.11.2",
    "@medusajs/cli": "2.11.2",
    "@medusajs/framework": "2.11.2",
    "@medusajs/test-utils": "2.11.2",
    "@medusajs/medusa": "2.11.2",
    "@medusajs/ui": "4.0.25",
    "@medusajs/icons": "2.11.2",
    "@medusajs/admin-shared": "2.11.2"
  },
  "engines": {
    "node": ">=20"
  }
}